public class Employee {

    protected double hours;
    protected int day;
    protected String role;
    protected double hourlyPay;
    protected double benefits;

    public Employee(double hours, int day, String role, double hourlyPay) {
        this.hours = hours;
        this.day = day;
        this.role = role;
        this.hourlyPay = hourlyPay;
    }

    public double getHours() {
        return hours;
    }

    public int getDay() {
        return day;
    }

    public String getRole() {
        return role;
    }

    public double getHourlyPay() {
        return hourlyPay;
    }

    public double getBenefits() {
        return benefits;
    }

    public double salary (double hours, double hourlyPay, int day) {
        double salary = hours * hourlyPay * day;
        return salary;
    }

    public 

    public void displayInfo() {
        System.out.println("\nRole: " + role);
        System.out.println("Number of hours worked: " + hours);
        System.out.println("Number of days worked: " + day);
        System.out.println("Hourly pay: " + hourlyPay);
        System.out.println("Your estimated salary is: " + salary(hours, hourlyPay, day));
    }
}
